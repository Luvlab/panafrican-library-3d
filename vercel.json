{
  "version": 2,
  "buildCommand": "echo 'Building Panafrican Library 3D'",
  "outputDirectory": ".",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
